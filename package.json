{
  "name": "@thorbens/jikan-api",
  "version": "2.0.0-alpha.6",
  "description": "Provides a javascript api client for the myanimelist jikan api (https://jikan.moe).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "docs": "rimraf public && typedoc --out ./public/ --mode file --excludePrivate --theme minimal"
  },
  "author": "Thorben Schiller <schillerthorben@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/thorbens/anime/jikan-api.git"
  },
  "keywords": [
    "anime",
    "myanimelist",
    "jikan",
    "api",
    "typescript"
  ],
  "dependencies": {
    "@thorbens/logger-model": "^1.0.0",
    "@thorbens/fetcher-model": "^1.0.0"
  },
  "devDependencies": {
    "@thorbens/axios-fetcher": "^1.1.0",
    "@types/jest": "^24.0.20",
    "@types/node": "^12.11.7",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-mockito": "^2.5.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  }
}
