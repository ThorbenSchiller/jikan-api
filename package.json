{
  "name": "@thorbens/jikan-api",
  "version": "2.0.0-alpha.1",
  "description": "Provides a javascript api client for the myanimelist jikan api (https://jikan.moe).",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "docs": "rimraf doc && typedoc --out ./public/ --mode file --excludePrivate --theme minimal"
  },
  "author": "Thorben Schiller <schillerthorben@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/thorbens/anime/jikan-api.git"
  },
  "keywords": [
    "anime",
    "myanimelist",
    "jikan",
    "api",
    "typescript"
  ],
  "dependencies": {
    "@thorbens/logger-model": "^1.0.0",
    "@thorbens/fetcher": "^3.0.0-alpha.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.12",
    "@types/whatwg-mimetype": "^2.1.0",
    "jest": "^23.6.0",
    "axios": "^0.18.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.1",
    "typescript": "^3.3.3"
  }
}
