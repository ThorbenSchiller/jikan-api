{
  "name": "@thorbens/jikan-api",
  "version": "2.0.0-alpha.9",
  "description": "Provides a javascript api client for the myanimelist jikan api (https://jikan.moe).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "docs": "rimraf public && typedoc --out ./public/ --mode file --excludePrivate --theme minimal"
  },
  "author": "Thorben Schiller <schillerthorben@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/thorbens/anime/jikan-api.git"
  },
  "keywords": [
    "anime",
    "myanimelist",
    "jikan",
    "api",
    "typescript"
  ],
  "dependencies": {
    "@thorbens/logger-model": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/node-fetch": "^2.5.6",
    "jest": "^25.1.0",
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.1",
    "ts-jest": "^25.2.0",
    "tslint": "^6.0.0",
    "typedoc": "^0.16.9",
    "typescript": "^3.7.5"
  }
}
